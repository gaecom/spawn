{
    "name": "liuggio/spawn",
    "description": "Concurrent processing of closures and commands in PHP with ease.",
    "license": "MIT",
    "authors": [
        {
            "name": "liuggio",
            "email": "liuggio@gmail.com"
        }
    ],
    "require": {
        "symfony/stopwatch": "^2.2",
        "symfony/process": "^2.2",
        "symfony/event-dispatcher": "^2.7",
        "jeremeamia/superclosure": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "Liuggio\\Spawn\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Liuggio\\Spawn\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-events": "0.0-dev"
        }
    }
}
