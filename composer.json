{
    "name": "liuggio/concurrent",
    "description": "Concurrent processing of closures and commands in PHP with ease.",
    "license": "MIT",
    "authors": [
        {
            "name": "liuggio",
            "email": "liuggio@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "~2.2",
        "symfony/stopwatch": "~2.2",
        "symfony/process": "~2.2",
        "symfony/event-dispatcher": "^2.7",
        "jeremeamia/superclosure": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "fabpot/php-cs-fixer": "~1.0"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {"Liuggio\\Concurrent\\":["src"]}
    },
    "autoload-dev": {
        "psr-4": {"Liuggio\\Concurrent\\":"tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-events": "0.0-dev"
        }
    }
}
